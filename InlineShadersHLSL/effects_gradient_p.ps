#include "common.h"

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main(p_TL I) : Sv_Target
{
	float3 color = I.Color.rgb;
	float vignette_mask = saturate(pow(1.f - distance(I.Tex0, 0.5f), 6.0f) * 1.15f);
	return float4(color * 1.1f, I.Color.w * 10.f) * vignette_mask;
};